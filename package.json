    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.8",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2",
    "ts-node": "^10.4.0",
    "webpack": "^5.72.0",
    "husky": "^7.0.0"
